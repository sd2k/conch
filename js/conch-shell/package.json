{
  "name": "@bsull/conch",
  "type": "module",
  "description": "A sandboxed bash-compatible shell running in WebAssembly, for browser and Node.js",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bsull/conch.git"
  },
  "files": [
    "interfaces/*",
    "shims/*",
    "*.wasm",
    "*.js",
    "*.d.ts",
    "README.md"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./vfs": {
      "types": "./vfs.d.ts",
      "default": "./vfs.js"
    },
    "./stdin": {
      "types": "./stdin.d.ts",
      "default": "./stdin.js"
    },
    "./tool-handler": {
      "types": "./tool-handler.d.ts",
      "default": "./tool-handler.js"
    },
    "./low-level": {
      "types": "./conch-shell.d.ts",
      "default": "./conch-shell.js"
    }
  },
  "keywords": [
    "shell",
    "bash",
    "wasm",
    "webassembly",
    "sandbox",
    "component-model"
  ],
  "dependencies": {
    "@bytecodealliance/preview2-shim": "^0.17.0"
  }
}
